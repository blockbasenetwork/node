{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    },
    "Serilog": {
      "MinimumLevel": {
        "Default": "Debug",
        "Override": {
          "Microsoft": "Information",
          "Microsoft.AspNetCore.Hosting": "Information",
          "Microsoft.AspNetCore.Mvc": "Information",
          "System": "Information"
        }
      }
    }
  },
  "AllowedHosts": "*",
  "NodeConfigurations": {
    "AccountName": "blockbase",
    "ActivePrivateKey": "__privatekey__",
    "ActivePublicKey": "",
    "SecretPassword": "secret",
    "MySqlServer": "localhost",
    "MySqlPort": 3306,
    "MySqlUser": "root",
    "MySqlPassword": "",
    "MongoDbConnectionString": "mongodb://localhost",
    "MongoDbPrefix": "blockbase"
  },
  "NetworkConfigurations": {
    "LocalIpAddress": "127.0.0.1",
    "LocalTcpPort": 4444,
    "EosNet": "http://127.0.0.1:8888",
    "ConnectionExpirationTimeInSeconds": 15,
    "MaxNumberOfConnectionRetries": 3,
    "BlockBaseOperationsContract": "blockbase",
    "BlockBaseTokenContract": "bbtoken"
  }
}
